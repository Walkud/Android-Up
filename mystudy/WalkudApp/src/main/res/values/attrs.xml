<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RoundRelativeLayout">
        <attr name="rfl_radius" format="dimension" />
        <attr name="topLeftRadius" format="dimension" />
        <attr name="topRightRadius" format="dimension" />
        <attr name="bottomLeftRadius" format="dimension" />
        <attr name="bottomRightRadius" format="dimension" />
        <attr name="border_color" format="color" />
        <attr name="border_size" format="dimension" />
    </declare-styleable>

    <declare-styleable name="RoundShadowView">
        <attr name="rsv_round_radius" format="dimension" />
        <attr name="rsv_bmf_radius" format="dimension" />
        <attr name="rsv_padding" format="dimension" />
        <attr name="rsv_color" format="color" />
    </declare-styleable>

    <declare-styleable name="PageGridView">
        <!--行数，如果为0表示横向显示gridview-->
        <attr name="PagingRows" format="integer" />
        <!--列数,如果为0表示纵向显示gridview-->
        <attr name="PagingColums" format="integer" />
        <attr name="PagingDiver" format="reference" />
    </declare-styleable>

</resources>