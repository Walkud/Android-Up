<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.walkud.jam">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
            android:name=".module.MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
        <activity
            android:name=".module.buildlayout.BuildLayoutActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".module.buildlayout.b.LinearLayoutActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".module.buildlayout.b.RelativeLayoutActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".module.makeapp.MakeAppActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".module.makeapp.a.BuyGoodsActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".module.makeapp.p2.BasketBallActivity"
            android:screenOrientation="portrait" />


    </application>

</manifest>
